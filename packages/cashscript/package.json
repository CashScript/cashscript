{
  "name": "cashscript",
  "version": "0.8.0-next.2",
  "description": "Easily write and interact with Bitcoin Cash contracts",
  "keywords": [
    "bitcoin cash",
    "cashscript",
    "sdk",
    "smart contracts"
  ],
  "homepage": "https://cashscript.org",
  "bugs": {
    "url": "https://github.com/Bitcoin-com/cashscript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bitcoin-com/cashscript.git"
  },
  "license": "MIT",
  "author": "Rosco Kalis <roscokalis@gmail.com>",
  "contributors": [
    "Gabriel Cardona <gabriel@bitcoin.com>"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:test": "yarn clean:test && yarn compile:test",
    "clean": "rm -rf ./dist",
    "clean:test": "rm -rf ./dist-test",
    "compile": "tsc -p tsconfig.build.json",
    "compile:test": "tsc -p tsconfig.test.json",
    "lint": "eslint . --ext .ts --ignore-path ../../.eslintignore",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "pretest": "yarn build:test",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest"
  },
  "dependencies": {
    "@bitauth/libauth": "^2.0.0-alpha.8",
    "@cashscript/utils": "^0.8.0-next.2",
    "bip68": "^1.0.4",
    "bitcoin-rpc-promise-retry": "^1.3.0",
    "delay": "^5.0.0",
    "electrum-cash": "^2.0.10"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@psf/bch-js": "^4.15.0",
    "bip39": "^3.0.4",
    "eslint": "^7.20.0",
    "jest": "^29.4.1",
    "typescript": "^4.1.5"
  },
  "gitHead": "bf02a4b641d5d03c035d052247a545109c17b708"
}
